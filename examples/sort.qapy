a = [secret(fmt('a[{}]', i)) for i in range(256)]
with a as r[256]:
    for i in range(256):
        for j in range(i + 1, 256):
            if b64(a[j]) < b64(a[i]):
                a[j], a[i] = a[i], a[j]
    return a
assert_is_perm(a, r, 'r is not a permutation of a')
R = [b64(r[i]) for i in range(256)]
for i in range(1, 256):
    assert_binle(R[i - 1], R[i], 'r is not sorted')
for i in range(256):
    reveal(fmt('r[{}]', i), r[i])
